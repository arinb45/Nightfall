config_version=5

[application]

config/name="NightFall"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://assests/Logo.png"
boot_splash/fullsize=false
config/icon="res://assests/textures/icon.png"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/viewport_height=720
window/size/mode=3
window/size/borderless=true
window/stretch/mode="viewport"
mouse_cursor/custom_image="res://assests/custom cursor.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/proton_scatter/plugin.cfg", "res://addons/terrain/plugin.cfg", "res://addons/terrain_3d/plugin.cfg")

